import utf.AssetPaths;
import utf.objects.room.Object;

class SolidSmallThinObject extends Object
{
	public function new():Void
	{
		super('solidsmallthin');

		loadGraphic(AssetPaths.sprite(objectID));
		scale.set(2, 2);
		updateHitbox();

		active = false;
		immovable = true;
		solid = true;
	}
}
