import flixel.FlxG;
import utf.AssetPaths;
import utf.objects.dialogue.typers.Typer;

class BattleTyper extends Typer
{
	public function new():Void
	{
		super('battle');

		fontName = AssetPaths.font('DTM-Mono');
		fontSize = 32;
		typerSounds = [FlxG.sound.load(AssetPaths.sound('txt2'), 0.86)];
		typerDelay = 0.05;
	}
}
