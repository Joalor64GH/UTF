import flixel.FlxG;
import utf.AssetPaths;
import utf.objects.dialogue.typers.Typer;

class GameOverTyper extends Typer
{
	public function new():Void
	{
		super('gameover');

		fontName = AssetPaths.font('DTM-Mono', false);
		fontSize = 32;
		fontSpacing = 2;
		typerSounds = [{sound: AssetPaths.sound('txt1', false), volume: 1}];
		typerLPS = 0.25;
	}
}
