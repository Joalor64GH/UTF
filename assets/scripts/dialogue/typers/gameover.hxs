import flixel.FlxG;
import utf.AssetPaths;
import utf.objects.dialogue.typers.Typer;

class GameOverTyper extends Typer
{
	public function new():Void
	{
		super('gameover');

		fontName = AssetPaths.font('DTM-Mono');
		fontSize = 32;
		fontSpacing = 2;
		typerSounds = [FlxG.sound.load(AssetPaths.sound('txt1'), 1.0)];
		typerLPS = 0.05;
	}
}
