import flixel.FlxG;
import utf.AssetPaths;
import utf.objects.dialogue.typers.Typer;

class DefaultTyper extends Typer
{
	public function new():Void
	{
		super('default');

		fontName = AssetPaths.font('DTM-Mono', false);
		fontSize = 32;
		typerSounds = [FlxG.sound.load(AssetPaths.sound('txt1', false), 1)];
		typerLPS = 0.05;
	}
}
